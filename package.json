{
  "name": "postgrester",
  "version": "1.0.0-alpha.2",
  "license": "Apache-2.0",
  "description": "PostgREST API Client.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">= 10.16.0"
  },
  "scripts": {
    "build": "rollup -c && copyfiles -f ./src/index.d.ts ./dist",
    "ci:generate:release_notes": "node ./scripts/ci/generate_release_notes.js",
    "test": "yarn test:lint && yarn test:unit",
    "test:lint": "tslint -p ./tsconfig.json",
    "test:unit": "jest",
    "test:watch": "yarn test:unit --watch",
    "preversion": "yarn test",
    "postversion": "git push --tags && git push origin HEAD"
  },
  "dependencies": {
    "axios": "0.19.0"
  },
  "devDependencies": {
    "@types/jest": "24.0.23",
    "copyfiles": "2.1.1",
    "coveralls": "3.0.9",
    "jest": "24.9.0",
    "keep-a-changelog": "0.8.1",
    "prettier": "1.19.1",
    "rollup": "1.27.8",
    "rollup-plugin-cleaner": "1.0.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-typescript2": "0.24.3",
    "ts-jest": "24.2.0",
    "tslint": "5.20.1",
    "tslint-config-airbnb": "5.11.2",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.7.3"
  },
  "resolutions": {
    "handlebars": ">=4.3.0",
    "semver": ">=4.3.0"
  },
  "keywords": [
    "api",
    "client",
    "postgres",
    "postgresql",
    "postgrest"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SocialGouv/postgrester.git"
  },
  "bugs": {
    "url": "https://github.com/SocialGouv/postgrester/issues"
  },
  "homepage": "https://github.com/SocialGouv/postgrester#readme"
}
