{
  "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["./src/**/*.test.ts"]
  },
  "rules": {
    "no-console": [true, "log"],
    "prettier": true,
    "variable-name": [true, "allow-pascal-case", "ban-keywords", "check-format"]
  },
  "rulesDirectory": ["tslint-plugin-prettier"]
}
